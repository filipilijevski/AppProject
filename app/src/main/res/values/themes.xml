    <style name="Theme.RentalAppCV" parent="Base.Theme.RentalAppCV" />

    <!--
         New style for a Translucent No Title Bar theme.
         If you need to reference @style/Theme.Translucent.NoTitleBar
         in your manifest for something like com.google.android.gms.common.api.GoogleApiActivity,
         this style will resolve that reference.
    -->
    <style name="Theme.Translucent.NoTitleBar" parent="@android:style/Theme.Translucent">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>

        <!-- Optionally make the status bar or nav bar translucent, etc. -->
        <!-- <item name="android:statusBarColor">@android:color/transparent</item> -->
    </style>

